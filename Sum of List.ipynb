    "def Sum(List):\n",
    "    total = 0\n",
    "    for i in List:\n",
    "        total += int(i)\n",
    "    return total\n",
    "\n",
    "L =list(input('List elements: ').split(' '))#Enter the elements with space in between\n",
    "print(Sum(L))"

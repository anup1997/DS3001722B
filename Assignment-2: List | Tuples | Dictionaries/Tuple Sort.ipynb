   "source": [
    "t =[(2, 5), (1, 2), (4, 4), (2, 3), (2, 1)]\n",
    "length = len(t)\n",
    "\n",
    "for i in range(0, length):\n",
    "    for j in range(0, length-i-1):\n",
    "        if (t[j][-1] > t[j + 1][-1]):\n",
    "            temp = t[j]\n",
    "            t[j]= t[j + 1]\n",
    "            t[j + 1]= temp\n",
    "            \n",
    "print(t)\n"
   ]
  }
